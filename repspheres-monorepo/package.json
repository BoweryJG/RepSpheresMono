{
  "name": "repspheres-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "npx turbo build --filter=./packages/*",
    "dev": "npx turbo dev",
    "lint": "npx turbo lint",
    "test": "npx turbo test",
    "format": "prettier --write \"**/*.{ts,tsx,md,js,jsx}\"",
    "build:types": "npx turbo build:types"
  },
  "devDependencies": {
    "prettier": "^3.0.0",
    "turbo": "^1.10.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@8.6.0"
}
